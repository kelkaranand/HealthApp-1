<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:id="@+id/imageView11"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/base"
        android:scaleType="centerCrop"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:id="@+id/imageView12"
        android:layout_alignParentBottom="true"
        android:scaleType="centerCrop"
        android:src="@drawable/base"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your Profile"
        android:textStyle="bold"
        android:textSize="30sp"
        android:textColor="#3399ff"
        android:id="@+id/textView11"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/imageView11"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Name :"
        android:id="@+id/textView18"
        android:layout_below="@+id/textView11"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:textSize="20sp"
        android:textColor="#3399ff"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Birth Date :"
        android:id="@+id/textView19"
        android:layout_below="@+id/textView18"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:textSize="20sp"
        android:textColor="#3399ff"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Gender :"
        android:id="@+id/textView20"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:layout_alignParentStart="true"
        android:textSize="20sp"
        android:textColor="#3399ff"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Devices :"
        android:id="@+id/textView21"
        android:layout_below="@+id/textView20"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:layout_alignParentStart="true"
        android:textSize="20sp"
        android:textColor="#3399ff"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/textView22"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@+id/textView18"
        android:layout_alignBaseline="@+id/textView18"
        android:textColor="#fdd017"
        android:textSize="15sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/textView23"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/textView19"
        android:layout_alignBaseline="@id/textView19"
        android:textColor="#fdd017"
        android:textSize="15sp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/textView24"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/textView20"
        android:layout_alignBaseline="@id/textView20"
        android:textColor="#fdd017"
        android:textSize="15sp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/textView25"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/textView21"
        android:layout_alignBaseline="@id/textView21"
        android:textColor="#fdd017"
        android:textSize="15sp"/>

    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/imageView18"
        android:src="@drawable/edit"
        android:layout_below="@id/textView21"
        android:layout_marginTop="70dp"
        android:layout_centerHorizontal="true"
        />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:id="@+id/imageView19"
        android:src="@drawable/add"
        android:layout_toRightOf="@id/textView25"
        android:layout_alignTop="@id/textView21"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="10dp"/>


</RelativeLayout>
